function vpn_height(){
  vpn_info_height = $('.vpn-info').outerHeight() - 6 + 29;
  $('.vpn-info-wrap .torrent-detail .shadow').css('bottom', - vpn_info_height)
}
  function resizeHeight() {
    jQuery('.js-height .js-col').maxHeight();
  }
