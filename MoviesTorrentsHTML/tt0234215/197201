    window_height = $(window).height();
    modal_height = $('.modal.in .modal-dialog').outerHeight();
    total_height = window_height - modal_height;
    $('.modal.in .modal-dialog').css('margin-top', total_height / 2);
  }
  
  $('.modal').on('shown.bs.modal', function (e) {
    modal_resize();
  })
